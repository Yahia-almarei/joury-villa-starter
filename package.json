{
  "name": "joury-villa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-deprecation\" next dev",
    "dev:clean": "rimraf .next && cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-deprecation\" next dev",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-deprecation\" next build",
    "start": "next start",
    "clean": "rimraf .next node_modules/.cache",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase db push",
    "supabase:seed": "supabase db reset --linked=false && psql postgresql://postgres:postgres@localhost:54322/postgres -f supabase/seed.sql",
    "supabase:types": "supabase gen types typescript --local --schema public > types/supabase.ts",
    "db:migrate": "npm run supabase:migrate",
    "db:seed": "npm run supabase:seed",
    "postinstall": "echo 'Using Supabase instead of Prisma'"
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@hookform/resolvers": "^5.2.1",
    "@next/bundle-analyzer": "^15.4.6",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.55.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.539.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.20",
    "nodemailer": "^6.10.1",
    "react": "18.2.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "resend": "3.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.19",
    "cross-env": "^10.0.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.41",
    "prisma": "^6.14.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "3.4.9",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "vitest": "1.6.0"
  }
}
